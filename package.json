{
  "name": "portfolio-website-desktop",
  "version": "1.0.0",
  "description": "Jayant Arora's Portfolio Website - Static site with AWS Amplify frontend and Serverless API",
  "main": "index.js",
  "scripts": {
    "build": "python app/index_standalone.py",
    "dev": "python app.py",
    "deploy:lambda": "serverless deploy",
    "deploy:lambda:prod": "serverless deploy --stage prod",
    "remove:lambda": "serverless remove"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "serverless": "^3.33.0",
    "serverless-domain-manager": "^7.1.0",
    "serverless-python-requirements": "^6.0.0",
    "serverless-s3-sync": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/majorjayant/portfolio-website-desktop.git"
  },
  "keywords": [
    "portfolio",
    "aws",
    "serverless",
    "amplify",
    "python"
  ],
  "author": "Jayant Arora",
  "license": "MIT"
}
